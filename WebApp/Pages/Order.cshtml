@page
@model WebApp.Pages.OrderModel
@{
	ViewData["Title"] = "Order page";
}
<div>
    <h2 class="display-4">Orders</h2>
</div>

<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>Order Id</th>
            <th>Product Id</th>
            <th>User Id</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model.Orders)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.ProductId</td>
                <td>@order.UserId</td>
            </tr>
        }
    </tbody>
</table>